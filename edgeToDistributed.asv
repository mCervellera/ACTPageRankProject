function y = edgeToDistributed( outLink, inLink, agentIndex , n )

Ad = zeros(n);

for i = 1:size(outLink)
    Ad(outLink(i),agentIndex) = 1 / size(outLink,1);
end
for i = 1:size(inLink)
    Ad(agentIndex, inLink(i,1)) = 1 / inLink(i,2);
end


